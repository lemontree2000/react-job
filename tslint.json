{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": [
      false
    ],
    "only-arrow-functions": [
      false
    ],
    "no-debugger": [false],
    "ordered-imports": [false],
    "jsx-no-lambda": [false],
    "object-literal-sort-keys":[false]
  }
}
